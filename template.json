{
  "name": "Docker + Oauth2.1 + Single user (no DB)",
  "description": "Single user authentication (= API key) while still implementing the OAuth2.1 protocol, as required by the MCP specs",
  "version": "1.0.0",
  "mcpressoVersion": "^0.7.0",
  "category": "docker",
  "authType": "oauth",
  "complexity": "easy",
  "features": [
    "MCP server",
    "TypeScript",
    "Production ready"
  ],
  "requirements": [
    "Node.js 18+",
    "npm or yarn"
  ],
  "envVars": [
    {
      "name": "PORT",
      "description": "Server port",
      "required": false,
      "default": "3000"
    },
    {
      "name": "SERVER_URL",
      "description": "Base URL of your server",
      "required": true
    }
  ]
}
